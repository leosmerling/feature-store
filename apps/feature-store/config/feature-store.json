{
    "app": {
        "name": "feature-store",
        "version": "${FEATURE_STORE_APP_VERSION}"
    },
    "plugins": [],
    "engine": {},
    "env": {},
    "settings": {
        "batch_storage.save": {
            "path": "${FEATURE_STORE_DATA_PATH}/${FEATURE_STORE_APP_VERSION}/{auto}",
            "partition_dateformat": "%Y/%m/%d/%H/",
            "flush_seconds": 60.0,
            "flush_max_size": 10000
        },
        "batch_storage.submit_query": {
            "seed_data_path": "${FEATURE_STORE_DATA_PATH}/${FEATURE_STORE_APP_VERSION}/{auto}"
        }
    },
    "events": {
        "create_feature": {
            "type": "POST"
        },
        "set_values": {
            "type": "POST",
            "write_stream": {
                "name": "{auto}"
            }
        },
        "online_storage.query": {
            "type": "POST"
        },
        "online_storage.save": {
            "type": "STREAM",
            "read_stream": {
                "name": "{events.set_values.write_stream.name}",
                "consumer_group": "{auto}"
            }
        },
        "batch_storage.save": {
            "type": "STREAM",
            "read_stream": {
                "name": "{events.set_values.write_stream.name}",
                "consumer_group": "{auto}"
            },
            "impl": "hopeit.parquet_storage.events.stream_batch_storage",
            "dataobjects": [
                "feature_store.datamodel.FeatureValue"
            ]
        },
        "batch_storage.submit_query": {
            "type": "POST"
        },
        "batch_storage.merge_query_results": {
            "type": "POST"
        }
    }
}
